  function transDate(list,idstr,pidstr){  
    var result = [],temp = {};  
    for(i = 0; i < list.length; i++){  
        temp[list[i][idstr]]=list[i]; 
    }  
    for(j=0; j<list.length; j++){  
        tempVp = temp[list[j][pidstr]];  
        if(tempVp){
            if(!tempVp["data"]) tempVp["data"] = [];
            tempVp["data"].push(list[j]); 
        }else{  
            result.push(list[j]); 
        }  
    }  
    return result;  
}  
  
console.log(transDate(data,"id","parent")); 
